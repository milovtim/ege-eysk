        </descr>
        <cite>
            <content link="http://ru.wikiquote.org/wiki/%D0%AD%D0%BB%D0%B1%D0%B5%D1%80%D1%82_%D0%93%D1%80%D0%B8%D0%BD_%D0%A5%D0%B0%D0%B1%D0%B1%D0%B0%D1%80%D0%B4">
